{
  "name": "@racedirector/iracing-sim-incident-emitter",
  "version": "0.0.1",
  "description": "EventEmitter that emits events when sim incidents are detected from iRacing.",
  "repository": {
    "type": "git",
    "url": "https://github.com/racedirector/iracing-socket-js.git",
    "directory": "packages/consumers/sim-incident-emitter"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint **/*.ts",
    "test": "jest --no-watchman",
    "prepack": "bob prepack"
  },
  "dependencies": {
    "@racedirector/iracing-socket-js": "0.1.0"
  },
  "peerDependencies": {
    "lodash": "^4"
  },
  "devDependencies": {
    "jest-websocket-mock": "^2.3.0",
    "lodash": "^4.17.21"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  }
}
