{
  "name": "@racedirector/iracing-socket-js",
  "version": "0.1.0",
  "description": "iRacingBrowserApps/Kapps socket adpater for iRacing",
  "repository": {
    "type": "git",
    "url": "https://github.com/racedirector/iracing-socket-js.git",
    "directory": "packages/iracing-socket-js"
  },
  "license": "MIT",
  "bin": {
    "iracing-socket": "dist/cli.js"
  },
  "scripts": {
    "lint": "eslint **/*.ts",
    "test": "jest --no-watchman",
    "prepack": "bob prepack"
  },
  "dependencies": {
    "ts-invariant": "^0.9.4",
    "winston": "^3.7.2",
    "yargs": "^17.4.1"
  },
  "peerDependencies": {
    "lodash": "^4",
    "react": "^17.0.0",
    "websocket-polyfill": "^0.0.3"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "websocket-polyfill": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/react": "^13.0.1",
    "@types/yargs": "^17",
    "jest-websocket-mock": "^2.3.0",
    "waait": "^1.0.5"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "bin": {
      "iracing-socket": {
        "input": "./src/cli.ts"
      }
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  }
}
