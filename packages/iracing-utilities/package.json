{
  "name": "@racedirector/iracing-utilities",
  "version": "0.0.1",
  "description": "Utilities for parsing the iRacing memory map contents",
  "repository": {
    "type": "git",
    "url": "https://github.com/racedirector/iracing-socket-js.git",
    "directory": "packages/iracing-utilities"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint **/*.ts",
    "test": "jest --no-watchman"
  },
  "dependencies": {
    "@racedirector/iracing-socket-js": "^0.1.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  }
}
